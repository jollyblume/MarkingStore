{
    "name": "jollyblume/marking-store",
    "description": "symfony/workflow compliant multi-tenant marking store",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolly Blume",
            "email": "jollyblume2@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/workflow": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "psr/log": "~1.1",
        "ramsey/uuid": "@dev",
        "symfony/event-dispatcher": "^4.2",
        "symfony/property-access": "^4.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JBJ\\Workflow\\MarkingStore\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JBJ\\Workflow\\": "../Workflow/lib",
            "JBJ\\Workflow\\MarkingStore\\Tests\\": "tests"
        }
    }
}
